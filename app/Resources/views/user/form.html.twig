{% extends 'base.html.twig' %}

{% block body %}

    <div>
        <form method="post" action="{{ action }}" novalidate="novalidate">
            <div>
                {{ dump(form.vars.value) }}
            </div>
            <div>{{ form_errors(form.email) }}{{ form_label(form.email) }}: {{ form_widget(form.email) }}</div>
            <div>{{ form_errors(form.username) }}{{ form_label(form.username) }}: {{ form_widget(form.username) }}</div>
            <div>{{ form_errors(form.password) }}{{ form_label(form.password) }}: {{ form_widget(form.password) }}</div>
            <div>{{ form_errors(form.age) }}{{ form_label(form.age) }}: {{ form_widget(form.age) }}</div>
            <input type="submit" value="Insert">
        </form>


    </div>

{% endblock %}